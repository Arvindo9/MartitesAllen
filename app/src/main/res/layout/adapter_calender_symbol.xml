<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable
            name="data"
            type="com.maritesallen.almanac2020.ui.dashboard.calender.calenderAdapter.calenderSymbolAdapter.CalenderSymbolAdapterViewModel" />
    </data>
<androidx.appcompat.widget.LinearLayoutCompat
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/icon"
        android:layout_width="@dimen/imageIconAlmanacCalender"
        android:layout_height="@dimen/imageIconAlmanacCalender"
        tools:srcCompat="@drawable/almanac_icons_01"
        android:layout_marginEnd="@dimen/marginCalenderIcon" />

</androidx.appcompat.widget.LinearLayoutCompat>
</layout>